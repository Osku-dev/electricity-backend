type PriceConnection {
  edges: [PriceEdge!]!
  pageInfo: PageInfo!
  stats: Stats
}

type PriceEdge {
  node: Price!
  cursor: String!
}

type Price {
  timestamp: String!
  value: Float!
  resolutionMinutes: String!
}

type PageInfo {
  startCursor: String
  endCursor: String
  hasPreviousPage: Boolean!
  hasNextPage: Boolean!
}

type Stats {
  minPrice: Float
  maxPrice: Float
  avgPrice: Float
}